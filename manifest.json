{
    "name": "Dario Moccia - Tracker Episodi",
    "version": "1.0",
    "description": "Tieni conto in automatico di quante volte hai guardato su youtube le serie di Dario Moccia di Natale a Tokyo, Lost in Tokyo e Natale a New York!",
    "options_page": "options.html",
    "permissions": [
        "storage",
        "history"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "manifest_version": 2
}